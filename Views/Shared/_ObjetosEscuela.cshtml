
<table class="table table-dark table-hover">
  <thead>
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Id</th>
        <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var obj in Model)
    {
        <tr>
            <td>@obj.Nombre</td>
            <td>@obj.Id</td>
            <td>@Html.ActionLink("Edit", "Edit", new { id=obj.Id })</td>
        </tr>
    }
  </tbody>
</table>