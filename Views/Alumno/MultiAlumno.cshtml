@model IEnumerable<Alumno>

@{
    ViewData["Title"] = "Información Alumnos";
    Layout= "Simple"; //Va a buscar en Shared Simple.cshtml
}

<h1>Alumnos</h1>
<!-- <partial name="_ObjetosEscuela" /> -->
<table class="table table-dark table-hover">
  <thead>
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Id</th>
        <th scope="col">Curso</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var obj in Model)
    {
        <tr>
            <td>@obj.Nombre</td>
            <td>@obj.Id</td>
            <td>@obj.CursoId</td>
            <td>@Html.ActionLink("Edit", "Edit", new { id=obj.Id })</td>
            <td>@Html.ActionLink("Delete", "Delete", new { id=obj.Id })</td>
        </tr>
    }
  </tbody>
</table>

